@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_CrmLayout.cshtml";
}

<br />
<button id="btn1" style="display:inline-block; inline-size:150px;">Button1</button>
<br />
<button id="btn2" style="display:inline-block; inline-size:150px;">Button2</button>

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script>
    $("#btn1").click(function () {
        swal("Hello World!");
    });
    $("#btn2").click(function () {
        swal("Good job!", "You clicked the button!", "success");
    });
</script>
