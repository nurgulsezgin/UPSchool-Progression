@model List<AppUser>
@{
    ViewData["Title"] = "ExcelStatic";
    Layout = "~/Views/Shared/_CrmLayout.cshtml";
}

<h1>Index</h1>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Adı Soyadı</th>
        <th>Mail</th>
        <th>Cinsiyet</th>
        <th>Sil</th>
        <th>Düzenle</th>
        <th>Detaylar</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name @item.SurName</td>
            <td>@item.Email</td>
            <td>@item.Gender</td>
            <td><a href="#" class="btn btnt-outline-danger">Sil</a></td>
            <td><a href="#" class="btn btnt-outline-success">Güncelle</a></td>
            <td><a href="#" class="btn btnt-outline-dark">Detaylar</a></td>
        </tr>
    }
</table>
<a href="/Register/Index/" type="submit" class="btn btn-primary me-2">Yeni Üye Kaydı</a>
